    <script>
        $(document).ready(function() {
            $.getJSON("/getmycart1", function(result) {
                $.each(result, function (i, field) {
                    $.each(field.products, function(j, prod) {
                        $("#productslist").append(
                        `<div class="col-md-6 col-xl-3">
                        <div class="card-box product-box">

                            <div class="product-img-bg">
                                <h1>`+field.products[0].title+`</h1>
                            </div>

                            <div class="product-info">
                                <div class="row align-items-center">
                                    
                                    <div class="col-auto">
                                        <div class="product-price-tag">
                                            $`+field.products[0].sale_price+`
                                        </div>
                                    </div>

                                    <div class="col-auto">
                                        <div>
                                            Quantity: `+field.products[0].quantity+`
                                        </div>
                                    </div>
                                </div> <!-- end row -->
                            </div> <!-- end product info-->
                        </div> <!-- end card-box-->
                    </div> <!-- end col-->`
                    );
                    });
            });

        });
    });
